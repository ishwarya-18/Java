import java.util.Scanner;
public class Main{
    public static void main(String[]args){
        System.out.println("Enter the number: ");
        Scanner a=new Scanner(System.in);
        int num1=a.nextInt();
        for (int i=1;i<=10;i++)
        System.out.println(num1+"x"+i+"="+i*num1);
    }
}
